{
  "name": "@node-agents-examples/sip-um-actually",
  "version": "0.0.0",
  "description": "A LiveKit Agent that hosts a game of 'Um, Actually' with contestants that call in, using OpenAI's capabilities to process and respond to voice calls",
  "engines": {
    "node": ">=22.0.0"
  },
  "type": "module",
  "license": "Apache-2.0",
  "author": "Michael Solati <michael.solati@livekit.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livekit-examples/node-agents-examples.git"
  },
  "bugs": {
    "url": "https://github.com/livekit-examples/node-agents-examples/issues"
  },
  "homepage": "https://github.com/livekit-examples/node-agents-examples/sip/um-actually#readme",
  "scripts": {
    "start": "dotenvx run -f .env.local -- tsx src/agent.ts dev",
    "setup:twilio": "dotenvx run -f .env.local -- tsx src/setup-twilio.ts",
    "setup:livekit": "dotenvx run -f .env.local -- tsx src/setup-livekit.ts",
    "lint": "npm-run-all -s lint:*",
    "lint:typescript": "tsc --noEmit",
    "lint:gts": "gts lint",
    "fix": "gts fix"
  },
  "dependencies": {
    "@livekit/rtc-node": "^0.13.8",
    "twilio": "^5.5.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.39.0"
  }
}